{
  "@context":{
    "schema": "http://schema.org/"
  },
  "@id": "https://nfdi4culture.de/id/E4255",
  "schema:event": {
    "@id": "EVENT_ID",
    "schema:name": "EVENT_NAME",
    "schema:location": "LOCATION_ID",
    "schema:temporalCoverage": {
      "@type": "schema:DateTime",
      "@value": "EVENT_BEGIN/EVENT_END"
    },
    "schema:alternateName": [
      {
        "@value": "EVENT_ALT_NAME"
      }
    ],
    "schema:subjectOf": [
      {
        "@type": "schema:ResearchProject",
        "@id": "PROJECT_ID"
      }
    ],
    "schema:superEvent": [
      {
        "@type": "schema:Event",
        "@id": "SERIAL_ID"
      }
    ],
    "schema:recordedIn": [
      {
        "@id": "SOURCE_ID"
      }
    ],
    "schema:performer": [
      {
        "@type": "schema:Person",
        "@id": "PERFORMER_ID",
        "schema:hasOccupation": "PERFORMER_ROLE"
      },
      {
        "@type": "schema:PerformingGroup",
        "@id" : "GROUP_ID",
        "schema:description" : "GROUP_ROLE"
      }
    ],
    "schema:workPerformed": [
      {
        "@id": "WORK_ID",
        "schema:author": [
          {
          "@type": "schema:Person",
          "@id": "COMPOSER_ID"
          }
        ]
      }
    ],
    "schema:url" : "EVENT_URL"
  }
}