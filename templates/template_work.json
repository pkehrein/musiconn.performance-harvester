{
  "@context":{
    "schema": "http://schema.org/"
  },
  "@id": "https://nfdi4culture.de/id/E4255",
  "schema:MusicComposition": {
    "@id": "WORK_ID",
    "schema:name": "WORK_NAME",
    "schema:composer": [
      {
        "@type": "schema:Person",
        "@id": "COMPOSER_ID"
      }
    ],
    "schema:description": [
      {
        "@value": "WORK_DESCRIPTION"
      }
    ],
    "schema:alternateName": [
      {
        "@value": "EVENT_ALT_NAME"
      }
    ],
    "schema:genre": [
      {
        "@id": "GENRE_ID"
      }
    ],
    "schema:includedComposition": [
      {
        "@type": "schema:MusicComposition",
        "@id": "COMPOSITION_ID"
      }
    ],
    "schema:subjectOf": [
      {
        "@type": "schema:event",
        "@id": "EVENT_ID"
      },
      {
        "@type": "schema:ResearchProject",
        "@id": "PROJECT_ID"
      }
    ],
    "schema:contributor": [
      {
        "@type": "schema:Person",
        "@id": "PERFORMER_ID"
      },
      {
        "@type": "schema:PerformingGroup",
        "@id" : "GROUP_ID"
      }
    ],
    "schema:url" : "WORK_URL"
  }
}